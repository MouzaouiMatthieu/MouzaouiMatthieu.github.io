---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  {% for page in site.pages %}
    {% unless page.url contains "404" or page.url contains "google" %}
      <url>
        <loc>{{ site.url }}{{ site.baseurl }}{{ page.url }}</loc>
        {% if page.date %}
          <lastmod>{{ page.date | date_to_xmlschema }}</lastmod>
        {% else %}
          <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        {% endif %}
        <changefreq>monthly</changefreq>
        <priority>0.8</priority>
      </url>
    {% endunless %}
  {% endfor %}
  {% for post in site.posts %}
    <url>
      <loc>{{ site.url }}{{ site.baseurl }}{{ post.url }}</loc>
      <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.7</priority>
    </url>
  {% endfor %}
  {% for teaching in site.teaching %}
    <url>
      <loc>{{ site.url }}{{ site.baseurl }}{{ teaching.url }}</loc>
      {% if teaching.date %}
        <lastmod>{{ teaching.date | date_to_xmlschema }}</lastmod>
      {% else %}
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
      {% endif %}
      <changefreq>yearly</changefreq>
      <priority>0.6</priority>
    </url>
  {% endfor %}
  {% for publication in site.publications %}
    <url>
      <loc>{{ site.url }}{{ site.baseurl }}{{ publication.url }}</loc>
      {% if publication.date %}
        <lastmod>{{ publication.date | date_to_xmlschema }}</lastmod>
      {% else %}
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
      {% endif %}
      <changefreq>yearly</changefreq>
      <priority>0.9</priority>
    </url>
  {% endfor %}
  {% for talk in site.talks %}
    <url>
      <loc>{{ site.url }}{{ site.baseurl }}{{ talk.url }}</loc>
      {% if talk.date %}
        <lastmod>{{ talk.date | date_to_xmlschema }}</lastmod>
      {% else %}
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
      {% endif %}
      <changefreq>yearly</changefreq>
      <priority>0.6</priority>
    </url>
  {% endfor %}
  {% for note in site.notes %}
    <url>
      <loc>{{ site.url }}{{ site.baseurl }}{{ note.url }}</loc>
      {% if note.date %}
        <lastmod>{{ note.date | date_to_xmlschema }}</lastmod>
      {% else %}
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
      {% endif %}
      <changefreq>monthly</changefreq>
      <priority>0.5</priority>
    </url>
  {% endfor %}
</urlset>